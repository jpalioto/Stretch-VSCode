{
  "name": "StretchTimer",
  "displayName": "Stretch Timer",
  "description": "A reminder to get up and stretch while you code.",
  "version": "0.0.1",
  "publisher": "jpalioto",
  "engines": {
    "vscode": "^0.10.1"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "keywords": [
    "cat",
    "ergonomics",
    "stretching",
    "blood clot",
    "don't die"
  ],
  "homepage": "https://github.com/jpalioto/Stretch-VSCode.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpalioto/Stretch-VSCode.git"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#99CCFF",
    "theme": "light"
  },
  "activationEvents": [
    "onCommand:extension.stretchTimer"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.stretchTimer",
        "title": "Stretch Timer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "pretest": "tsc",
    "test": "mocha --ui tdd ./out/test/extension.test.js"
  },
  "devDependencies": {
    "expect": "^1.13.0",
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "open": "0.0.5"
  }
}
